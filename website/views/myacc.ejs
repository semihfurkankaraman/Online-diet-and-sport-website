<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Freelancer - Start Bootstrap Template</title>

    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Lato:wght@400;700&display=swap" rel="stylesheet">
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Lato:wght@400;700&family=Montserrat:wght@400;700&display=swap"
        rel="stylesheet">
    <link rel="stylesheet" href="https://pro.fontawesome.com/releases/v5.10.0/css/all.css"
        integrity="sha384-AYmEC3Yw5cVb3ZcuHtOA93w35dYTsvhLPVnYs9eStHfGJvOvKxVfELGroGkvsg+p" crossorigin="anonymous" />
    <link rel="stylesheet" href="/css/myacc.css">
</head>


<body>

    <%- include("partials/nav") %>


    

        <header>

            <h1 class="myaccount">My Account</h1>
            <a href="/myacc/diet">Diet Programs</a>
            <a href="/myacc/sport">Sport Programs</a>
            <a href="/myacc/orders">Orders</a>
            <a href="/logout">Log Out</a>

            <div class="box-container">
   
                    <% if (typeof orders !="undefined") { %>  
                           
                            <% orders.forEach(element=> { %>
                                <div class="box">

                                    <div class="order_id">
                                        <p>
                                            Orders Id : <%= element.order_id %>

                                        </p>
                                        
                                    </div>

                                    <div class="order_products">
                                        <p>
                                            Contents : <%= element.products%>
                                        </p>
                                        
                                    </div>

                                    <div class="total_cost">
                                        <p>
                                           Total Cost : <%= element.total_cost %>
                                        </p>
                                       
                                    </div>
                                    
                                    

                            </div>
                                
                            <% }) %>

                        
                         
                       
                    <% } %>
                


            </div>

        </header>



        <%- include("partials/footer") %>

</body>

</html>